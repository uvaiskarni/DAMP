    "import numpy as np\n",
    "def binary_sampler(p, rows, cols):\n",
    "    random_matrix = np.random.uniform(0., 1., size = [rows, cols])\n",
    "    binary_random_matrix = 1*(random_matrix < p)\n",
    "    return binary_random_matrix"
